{
  "name": "keep-learning-tokyo-lambda",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/RyuyaIshibashi/keep-learning-tokyo-serverless",
  "author": "ryuya ishibashi",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/local.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/local.js",
    "generate:error": "cross-env NODE_ENV=production GENERATE_ERROR_PAGE=true nuxt generate",
    "lint": "vue-cli-service lint --no-fix .",
    "lint:fix": "vue-cli-service lint .",
    "precommit": "npm run lint",
    "sls": "sls",
    "sls:deploy": "sls deploy --force",
    "deploy": "cross-env NODE_ENV=production ENDPOINT_ENV=cf node bin/deploy/index.js",
    "deploy:api_gw": "cross-env NODE_ENV=production ENDPOINT_ENV=api_gw run-s build sls:deploy",
    "delete": "sls remove"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@mdi/font": "^6.5.95",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/dotenv": "^1.4.1",
    "aws-serverless-express": "^3.4.0",
    "babel-runtime": "^6.26.0",
    "contentful": "^9.1.5",
    "core-js": "^3.20.0",
    "cross-env": "^7.0.2",
    "express": "^4.16.4",
    "moment-timezone": "^0.5.31",
    "node-sass": "^4.14",
    "nuxt": "^2.15.8",
    "vue-code-highlight": "^0.7.8",
    "vue-markdown": "^2.1.2",
    "vue-side-catalog": "^2.0.0",
    "vuetify": "^2.5.5"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@nuxtjs/eslint-config": "^8.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/google-fonts": "^1.3.0",
    "@nuxtjs/style-resources": "^1.2.1",
    "@nuxtjs/vuetify": "^1.12.1",
    "@vue/cli-plugin-eslint": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.2.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.5",
    "css-loader": "^5.1.1",
    "eslint": "7.32.0",
    "eslint-plugin-nuxt": "^3.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "jest": "^27.0.5",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.3",
    "postcss": "^8.1.10",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^4.2.0",
    "postcss-url": "^10.1.1",
    "sass-loader": "10.1.1",
    "serverless": "^2.68.0",
    "vue-jest": "^3.0.4"
  }
